datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TicketBooking {
  createdAt     DateTime  @default(now())
  eventDate     DateTime?
  id            String    @id @default(cuid())
  paymentStatus Boolean?
  timeSlot      String?
  updatedAt     DateTime  @updatedAt
}

model User {
  createdAt   DateTime @default(now())
  email       String?  @unique
  firstName   String?
  id          String   @id @default(cuid())
  lastName    String?
  password    String
  phoneNumber String?
  roles       Json
  updatedAt   DateTime @updatedAt
  username    String   @unique
}
